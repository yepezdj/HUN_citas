                                                    <div class="form-input">
                                                        <label for="Departamento" style="margin-top: 20px;"
                                                            class="form-label">Lugar de expedición (Departamento)<label
                                                                class="redText">*</label></label>
                                                        <input class="form-control" id="Departamento"
                                                            name="Departamento" type="text" autocomplete="off"
                                                            onkeypress="return false" value="<%= datos.Departamento %>"
                                                            readonly>
                                                    </div>
                                                </div>
