                                                        <input type="number" class="form-control" id="cellphone1"
                                                            name="cellphone1"
                                                            placeholder="Sin puntos ni comas ni guiones" min="0"
                                                            step="1" oninput="validity.valid||(value='');"
                                                            value="<%= datos.CelularOp %>">
                                                    </div>
                                                </div>
